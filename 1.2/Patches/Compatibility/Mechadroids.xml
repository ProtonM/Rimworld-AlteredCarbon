<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Mechadroids</li>
		</mods>
		<success>Always</success>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName='O21_Alien_MechadroidAlpha']/modExtensions</xpath>
					<order>Append</order>
					<value>
						<li Class="AlteredCarbon.ExcludeRacesModExtension">
							<canBeGrown>false</canBeGrown>
							<acceptsStacks>false</acceptsStacks>
						</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName='O21_Alien_MechadroidDelta']/modExtensions</xpath>
					<order>Append</order>
					<value>
						<li Class="AlteredCarbon.ExcludeRacesModExtension">
							<canBeGrown>false</canBeGrown>
							<acceptsStacks>false</acceptsStacks>
						</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName='O21_Alien_MechadroidGamma']/modExtensions</xpath>
					<order>Append</order>
					<value>
						<li Class="AlteredCarbon.ExcludeRacesModExtension">
							<canBeGrown>false</canBeGrown>
							<acceptsStacks>false</acceptsStacks>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>