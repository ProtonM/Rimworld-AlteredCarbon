<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef ParentName="SurgeryInstallImplantBase">
        <defName>AC_InstallEmptyCorticalStack</defName>
        <label>install empty cortical stack</label>
        <description>Install an empty cortical stack.</description>
        <descriptionHyperlinks>
            <ThingDef>AC_EmptyCorticalStack</ThingDef>
        </descriptionHyperlinks>
        <workerClass>AlteredCarbon.Recipe_InstallCorticalStack</workerClass>
        <jobString>Installing cortical stack.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>AC_EmptyCorticalStack</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>AC_EmptyCorticalStack</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Neck</li>
        </appliedOnFixedBodyParts>
        <addsHediff>AC_CorticalStack</addsHediff>
		<modExtensions>
            <li Class="AlteredCarbon.ExcludeRacesModExtension">
                <racesToExclude>
                    <li MayRequire="Atlas.AndroidTiers">Android1Tier</li>
                    <li MayRequire="Atlas.AndroidTiers">Android2Tier</li>
                    <li MayRequire="Atlas.AndroidTiers">Android3Tier</li>
                    <li MayRequire="Atlas.AndroidTiers">Android4Tier</li>
                    <li MayRequire="Atlas.AndroidTiers">Android5Tier</li>
                    <li MayRequire="Atlas.AndroidTiers">M7Mech</li>
