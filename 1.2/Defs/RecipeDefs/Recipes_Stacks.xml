                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android2KTX</li>
                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android2KITX</li>
                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android3TX</li>
                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android3ITX</li>
                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android4TX</li>
                    <li MayRequire="aRandomKiwi.AndroidTiersTXSerie">ATPP_Android4ITX</li>
                    <li MayRequire="neceros.androidsexpanded">NMedicDrone</li>
                    <li MayRequire="neceros.androidsexpanded">NEngiDrone</li>
                    <li MayRequire="neceros.androidsexpanded">NChefDrone</li>
